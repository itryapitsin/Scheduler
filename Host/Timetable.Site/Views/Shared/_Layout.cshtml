@using System.Web.Optimization
<!DOCTYPE html>
<html>
	<head>
	    <script>
    	    var pathPrefix = "@HttpContext.Current.Request.ApplicationPath";
    	</script>

		<meta http-equiv="Content-type" content="text/html; charset=utf-8">
		<meta name="viewport" content="width=device-width" />		
        <title>@ViewBag.Title</title>    
        @Styles.Render("~/Content/jquery")
        @Styles.Render("~/Content/bootstrap")
        @Styles.Render("~/Content/resources/css/ext-all.css")
		@RenderSection("css", false)
		
        @Scripts.Render("~/Scripts/jquery")
        @Scripts.Render("~/Scripts/knockout")
        @Scripts.Render("~/Scripts/bootstrap")
        @Scripts.Render("~/Scripts/Site/newbase.viewmodel.js")
        @Scripts.Render("~/Scripts/Site/data.model.js")
        @Scripts.Render("~/Scripts/ext-all.js")
		@RenderSection("scripts", false)
	</head>
	<body>
        @RenderSection("top", false)
		@RenderBody()
        @RenderSection("bottom", false)
	</body>
</html>
