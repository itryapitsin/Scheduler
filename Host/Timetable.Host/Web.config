<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=5.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <connectionStrings>
    <add name="SchedulerContext" providerName="System.Data.SqlClient" connectionString="Data Source=.\; Initial Catalog=TimetableDb-3; Integrated Security=true; MultipleActiveResultSets=true;" />
    <add name="UserContext" providerName="System.Data.SqlClient" connectionString="Data Source=.\; Initial Catalog=UsersDb; Integrated Security=true; MultipleActiveResultSets=true;" />
  </connectionStrings>
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <sessionState mode="Off" />
    <authentication mode="Windows"></authentication>
  </system.web>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="DataServiceBehavior">
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
        <behavior name="UserServiceBehavior">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <!--<protocolMapping>
      <add binding="basicHttpsBinding" scheme="https" />
    </protocolMapping>-->
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true" minFreeMemoryPercentageToActivateService="0" />
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpEndpointBinding" allowCookies="true" maxReceivedMessageSize="20000000" maxBufferSize="20000000" maxBufferPoolSize="20000000">
          <security mode="None" />
          <readerQuotas maxDepth="32" maxArrayLength="200000000" maxStringContentLength="200000000" />
        </binding>
        <binding name="BasicAnonymHttpEndpointBinding">
          <security mode="None" />
        </binding>
      </basicHttpBinding>
    </bindings>
    <services>
      <service behaviorConfiguration="DataServiceBehavior" name="Timetable.Host.Services.DataService">
        <endpoint binding="basicHttpBinding" bindingConfiguration="BasicHttpEndpointBinding" name="DataServiceEndpoint" contract="Timetable.Host.Interfaces.IDataService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
      </service>
      <service behaviorConfiguration="UserServiceBehavior" name="Timetable.Host.Services.UserService">
        <endpoint binding="basicHttpBinding" bindingConfiguration="BasicAnonymHttpEndpointBinding" name="UserServiceEndpoint" contract="Timetable.Host.Interfaces.IUserService" />
      </service>
    </services>
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" />
    <!--
        To browse web app root directory during debugging, set the value below to true.
        Set to false before deployment to avoid disclosing web app folder information.
      -->
    <directoryBrowse enabled="true" />
  </system.webServer>
  <system.runtime.serialization>
    <dataContractSerializer>
      <declaredTypes>
        <add type="Timetable.Data.Models.Scheduler.BaseIIASEntity, Timetable.Data, Version=1.0.0.0, Culture=neutral">
          <knownType type="Timetable.Data.Models.Scheduler.Auditorium, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Building, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Branch, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Course, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Department, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Faculty, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Group, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Lecturer, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Position, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Schedule, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.ScheduleInfo, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Speciality, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Time, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.Tutorial, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.TutorialType, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
          <knownType type="Timetable.Data.Models.Scheduler.WeekType, Timetable.Data, Version=1.0.0.0, Culture=neutral" />
