 <!-- Header button menu -->
   
     <div  style="left:5px; width:100%; height: 35px;z-index:1001; position:fixed; background-color:white;margin-top:0px; margin-bottom:20px; float:left;">
         
         <div style="padding-left:5px; width:243px; padding-right:5px; padding-top:5px; height: 35px; border:solid 1px; border-color:lightgray; float:left; background-color:white;">
         <!--<select style="top:10px; width:130px;"  data-placeholder="Выберите расписание" class="span5" id="timetable" data-bind="options: $root.timetables, optionsText: 'Name', value: $root.currentTimetable">
                <option></option>
         </select>
         

         <input type="button" id="enableweb" class="btn" value="Разр. Web" data-bind="click: $root.TimetableEnableWebButton" />
         <input type="button" id="disableweb" class="btn" value="Запр. Web" data-bind="click: $root.TimetableDisableWebButton" />
          -->
         </div>
         
         <div style="padding-left:5px; padding-right:5px; padding-top:5px; height: 35px; border:solid 1px; border-color:lightgray; float:left; background-color:white;">
         <select style="top:10px; width:110px;"  data-placeholder="Выберите учебный год" class="span5" id="studyyear" data-bind="options: $root.studyyears, optionsText: 'Name', value: $root.currentStudyYear">
                <option></option>
         </select>

         <select style="top:10px; width:50px;"  data-placeholder="Выберите семестр" class="span5" id="semester" data-bind="options: $root.semesters, optionsText: 'Name', value: $root.currentSemester">
                <option></option>
         </select>
         </div>

         <div style="padding-left:5px; padding-right:5px; padding-top:5px; height: 35px; border:solid 1px; border-color:lightgray; float:left; background-color:white;">
         <select style="top:10px; width:110px;"  data-placeholder="Выберите здание" class="span5" id="buildings1" data-bind="options: $root.buildings, optionsText: 'Name', value: $root.currentBuilding">
                <option></option>
         </select>
        
         <select style="width:130px;"  data-placeholder="Выберите тип аудитории" class="span5" id="auditoriumtype1" data-bind="options: $root.auditoriumTypes, optionsText: 'Name', value: $root.currentAuditoriumType">
            <option></option>
         </select>
         <select style="width:80px;"  data-placeholder="Выберите аудиторию" class="span5" id="auditoriumheader1" data-bind="options: $root.auditoriums, optionsText: 'Number', value: $root.currentAuditorium">
            <option></option>
         </select>
         <select style="width:53px;"  data-placeholder="Выберите тип недели" class="span5" id="weektype31" data-bind="options: $root.weekTypes, optionsText: 'Name', value: $root.currentWeekType">
            <option></option>
         </select>
         </div>

         <div style="padding-left:5px; padding-right:5px; padding-top:5px; height: 35px; border:solid 1px; border-color:lightgray; float:left; background-color:white;">
        <input type="button" id="add" class="btn disabled" value="Запланировать" data-bind="click: $root.SchedulePlanButton" />
        <input type="button" id="del" class="btn disabled" value="Снять занятие" data-bind="click: $root.ScheduleDeleteButton" />
        <!--<input type="button" id="change" class="btn disabled" value="Сделать замену" data-bind="click: $root.ScheduleChangeButton" />-->
        <input type="button" id="changeAuditorium" class="btn disabled" value="Поменять ауд." data-bind="click: $root.AuditoriumChangeButton" />
        <input type="button" id="freeAuditorium" class="btn disabled" value="Доступные ауд." data-bind="click: $root.FreeAuditoriumsButton" />
        <input type="button" id="printSchedule" class="btn" value="Печать" data-bind="click: $root.PrintScheduleButton" />
        <input type="button" id="printScheduleForGroups" class="btn" value="Печать по группам" data-bind="click: $root.PrintScheduleForGroupsButton" />
        </div>
     </div>
